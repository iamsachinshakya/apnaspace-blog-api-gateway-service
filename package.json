{
  "name": "api-gateway",
  "version": "1.0.0",
  "description": "A Node.js/Express API Gateway service for routing, rate-limiting, and proxying requests to microservices.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only --env-file=.env src/index.ts",
    "build": "tsc",
    "start": "node --env-file=.env dist/index.js",
    "lint": "eslint . --ext .ts"
  },
  "keywords": [
    "api-gateway",
    "express",
    "typescript",
    "microservices",
    "routing",
    "rate-limit",
    "proxy",
    "redis",
    "logging"
  ],
  "author": "Your Name <youremail@example.com>",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/api-gateway.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/api-gateway/issues"
  },
  "homepage": "https://github.com/yourusername/api-gateway#readme",
  "packageManager": "pnpm@10.18.3",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.2.1",
    "http-proxy-middleware": "^3.0.5",
    "morgan": "^1.10.1",
    "redis": "^4.7.0",
    "uuid": "^13.0.0",
    "winston": "^3.18.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/morgan": "^1.9.10",
    "@types/node": "^22.10.0",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
